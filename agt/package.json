{
  "name": "agt",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --ext .ts,.tsx,.vue",
    "lint:fix": "eslint . --ext .ts,.tsx,.vue --fix",
    "format": "prettier --check \"src/**/*.{ts,vue,js,jsx,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,vue,js,jsx,tsx}\"",
    "check": "npm run lint && npm run type-check && npm run format",
    "fix": "npm run lint:fix && npm run type-check && npm run format:fix"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.32.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-airbnb": "^8.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "typescript": "5.5",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
